    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "mic-2-canvas-1-speaking-voices-annotation-3.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Oana's voice, speaking with a grave tone and relatively low pitch. Many phrases are broken up into groups of 2-4 words, which are spoken slowly with pauses between groups of words. Short i sounds, as in \"grip\" and \"abyss,\" are extended, as are s sounds in the middle/end of words. Volume decreases and onset/offset softens as she speaks. ",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": {
          "id": "https://sarah-freeman.github.io/reflexive-annotation/mic-2/canvas-1/canvas",
